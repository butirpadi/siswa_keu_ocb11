<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_pembayaran_siswa" model="ir.sequence">
            <field name="name">Pembayaran Siswa</field>
            <field name="code">pembayaran.siswa.ocb11</field>
            <field name="prefix">PAY/%(range_year)s/%(range_month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_siswa_keu_kas" model="ir.sequence">
            <field name="name">Kas</field>
            <field name="code">siswa.keu.ocb11.kas</field>
            <field name="prefix">CASH/%(range_year)s/%(range_month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- <record id="default_kategori_kas" model="siswa_keu_ocb11.kas_kategori">
            <field name="name">Pendapatan Biaya Kesiswaan</field>
            <field name="tipe">in</field>            
        </record> -->

    </data>
</odoo>
