<odoo>
    <data>

        <record id="siswa_keu_ocb11.wizard_report_kas_form" model="ir.ui.view">
            <field name="name">wizard.ocb11.report_kas</field>
            <field name="model">siswa_keu_ocb11.wizard_report_kas</field>
            <field name="arch" type="xml">
                <form string="Wizard Report Kas" class="o_sale_order" create="false">
                    <sheet>
                        <div class="oe_title oe_read_only">
                            <h1>
                                <field name="name" readonly="1"/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="awal"  />                    
                            </group>
                            <group>
                                <field name="akhir" /> 
                            </group>
                        </group>
                        <footer class="oe_edit_only" >
                            <button name="action_save" type="object"
                                string="Print" class="oe_highlight"/>
                                
                            <button special="cancel" string="Cancel"/>
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>

        <act_window id="siswa_keu_ocb11.wizard_report_kas"
            name="Report Kas"
            src_model="siswa_keu_ocb11.kas"
            res_model="siswa_keu_ocb11.wizard_report_kas"
            view_mode="form"
            target="new"
            key2="client_action_multi"/>
    </data>
</odoo>